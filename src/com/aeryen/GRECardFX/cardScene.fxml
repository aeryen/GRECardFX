<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" vgap="10" hgap="10" stylesheets="/com/aeryen/GRECardFX/cardScene.css"
          xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.aeryen.GRECardFX.Controller">
    <children>
        <Label fx:id="detail" text="Incredible" GridPane.rowIndex="0" GridPane.columnIndex="0" GridPane.columnSpan="4"/>
        <Button fx:id="option1" prefHeight="17.0" text="磨损，刮除" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
        <Button fx:id="option2" prefHeight="17.0" text="并列地，并排地" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
        <Button fx:id="option3" prefHeight="17.0" text="删减；缩短" GridPane.rowIndex="1" GridPane.columnIndex="2"/>
        <Button fx:id="option4" prefHeight="17.0" text="废止，废除" GridPane.rowIndex="1" GridPane.columnIndex="3"/>
    </children>
    <columnConstraints>
        <ColumnConstraints/>
        <ColumnConstraints/>
        <ColumnConstraints/>
        <ColumnConstraints/>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
    </rowConstraints>

</GridPane>
